{{set root=".."}}
{{set pageId="mypage"}}
{{> header title='마이 - 좋아요 : 얍플레이스(YAP Place)' cssName='mypage.like'}}

<div id="mypage-contents">
	<div class="profile-cover">
		<div class="mypage-container">
			<div class="body">
				<div class="my-profile">
					<div class="user-img"></div>
					<div class="user-info">
						<div class="user-name">
							100suhyunqqqqqqqqq
						</div>
						<div class="user-sns">
							<div class="following">
								팔로잉 0
							</div>
							<div class="follower">
								팔로워 0
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="mypage-body mypage-container">
		<div class="body">
			<ul class="mypage-menu">
				{{#each (getObj pageId)}}
					<li class="{{class}}">
						{{title}}
						<span>
							{{count}}
						</span>
					</li>
				{{/each}}
			</ul>
			<div class="content-box">
				<div class="mypage-filter">
					<ul class="mypage-category">
						<li class="my-like-store">매장</li>
						<li class="my-like-theme">테마</li>
						<li class="my-like-products active">상품</li>
					</ul>
				</div>
				<div class="content-body">
					<div class="mypage-default-text">
						마음에 드는 상품을 좋아요 해보세요.
					</div>
					<div class="my-list">

					</div>
				</div>
			</div>
		</div>
	</div>
</div>



{{> footer jsName='mypage.common'}}
