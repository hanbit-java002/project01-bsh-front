{{set root=".."}}
{{set pageId="mypage"}}
{{> header title='마이 - 주문 : 얍플레이스(YAP Place)' cssName='mypage.order'}}

<div id="mypage-contents">
	<div class="profile-cover">
		<div class="mypage-container">
			<div class="body">
				<div class="my-profile">
					<div class="user-img"></div>
					<div class="user-info">
						<div class="user-name">
							100suhyunqqqqqqqqq
						</div>
						<div class="user-sns">
							<div class="following">
								팔로잉 0
							</div>
							<div class="follower">
								팔로워 0
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="mypage-body mypage-container">
		<div class="body">
			<ul class="mypage-menu">
				{{#each (getObj pageId)}}
					<li class="{{class}}">
						{{title}}
						<span>
							{{count}}
						</span>
					</li>
				{{/each}}
			</ul>
			<div class="content-box">
				<div class="mypage-filter">
					<ul class="mypage-category">
						<li class="my-order-history">주문 내역</li>
						<li class="my-order-review active">상품 후기</li>
						<li class="my-order-inquiry">상품 문의</li>
					</ul>
				</div>
				<div class="content-body">
					<div class="mypage-default-text">
						상품을 주문하고 소중한 후기를 남겨주세요.
					</div>
					<div class="my-list">

					</div>
				</div>

			</div>
		</div>
	</div>
</div>



{{> footer jsName='mypage.common'}}
